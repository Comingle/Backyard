<p id="notice"><%= notice %></p>

<h1>Listing Nunchucks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nunchucks.each do |nunchuck| %>
      <tr>
        <td><%= link_to 'Show', nunchuck %></td>
        <td><%= link_to 'Edit', edit_nunchuck_path(nunchuck) %></td>
        <td><%= link_to 'Destroy', nunchuck, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nunchuck', new_nunchuck_path %>
