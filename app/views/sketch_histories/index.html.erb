<p id="notice"><%= notice %></p>

<h1>Listing Sketch Histories</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sketch_histories.each do |sketch_history| %>
      <tr>
        <td><%= link_to 'Show', sketch_history %></td>
        <td><%= link_to 'Edit', edit_sketch_history_path(sketch_history) %></td>
        <td><%= link_to 'Destroy', sketch_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sketch history', new_sketch_history_path %>
